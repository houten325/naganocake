<div class="row">
<thead>
  <tr>
    <th>商品ID</th>
    <th>商品名</th>
    <th>税抜価格</th>
    <th>ジャンル</th>
    <th>ステータス</th>
  </tr>
</thead>

<tbody>
  <%= @items.each do |item| %>
   <tr>
     <td><%= item.id %></td>
     <td><%= link_to item.name ,admins_items_path(item) %></td>
     <td><%= item.price.to_s　%></td>
     <td><%= item.genre.name %></td>
     <td><% if item.is_active == false %></td>
     販売中
     <% else %>
     販売停止中
     <% end %>
     <%= pagenate @items %>
   </tr>
</tbody>
</div>